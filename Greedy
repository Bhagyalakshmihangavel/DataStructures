#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    
    int x;
    scanf("%d",&x);
    
    int max=arr[0];
    int min=arr[0];
    
    for(int i=0;i<n;i++)
    {
        if(arr[i]>max)
        {
            max=arr[i];
        }
    }
    
    for(int i=0;i<n;i++)
    {
        if(arr[i]<min)
        {
            min=arr[i];
        }
    }
    
    int c=0;
    
    while(x!=0){
        if(max<=x)
        {
            x=x-max;
            c++;
        }
        else
        {
          x=x-min;
          c++;
        }
    }
    printf("%d",c);
}

